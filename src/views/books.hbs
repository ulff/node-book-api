<table class="table">
    <thead>
    <tr>
        <th><a href="{{sortLinks.sortById}}">#</a></th>
        <th><a href="{{sortLinks.sortByTitle}}">Title</a></th>
        <th>Authors</th>
        <th>ISBN</th>
    </tr>
    </thead>
    <tbody>
    {{#books}}
        <tr>
            <th></th>
